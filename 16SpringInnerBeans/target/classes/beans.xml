<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="car" class="com.cloudsmartz.SpringListOfBeans.Car">
		<constructor-arg>
			<list>
				<bean class="com.cloudsmartz.SpringListOfBeans.Wheel">
					<property name="name">
						<value>MRF</value>
					</property>
					<property name="type">
						<value>TUBELESS</value>
					</property>
				</bean>

				<bean class="com.cloudsmartz.SpringListOfBeans.Wheel">
					<property name="name">
						<value>CEAT</value>
					</property>
					<property name="type">
						<value>TUBE</value>
					</property>
				</bean>

				<bean class="com.cloudsmartz.SpringListOfBeans.Wheel">
					<property name="name">
						<value>MRF</value>
					</property>
					<property name="type">
						<value>TUBELESS</value>
					</property>
				</bean>

				<bean class="com.cloudsmartz.SpringListOfBeans.Wheel">
					<property name="name">
						<value>MRF</value>
					</property>
					<property name="type">
						<value>TUBELESS</value>
					</property>
				</bean>

			</list>
		</constructor-arg>
	</bean>


</beans>